<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="TextButton" Color="#FF5E6787"/>
    <SolidColorBrush x:Key="DarkerTextButton" Color="#FF4C536B"/>
    <SolidColorBrush x:Key="NotWhite" Color="#FFD1D5D9"/>
    <SolidColorBrush x:Key="MyBlue" Color="#FF008CFF"/>
    <SolidColorBrush x:Key="TopMenu" Color="#FF202225"/>
    <LinearGradientBrush x:Key="background" StartPoint="0, 0" EndPoint="1, 1">
        <GradientStop Color="#3124F0" Offset="0.0"/>
        <GradientStop Color="#70A2FB" Offset="0.5"/>
        <GradientStop Color="#AE6FDC" Offset="1"/>
    </LinearGradientBrush>

</ResourceDictionary>